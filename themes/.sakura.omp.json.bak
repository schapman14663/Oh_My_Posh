{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": true,
  "version": 2,
  "auto_upgrade": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "shell",
          "style": "diamond",
          "leading_diamond": "\u256d\u2500\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "#4C4C4C",
          "background": "#CCFBFE",
          "properties": {
            "mapped_shell_names": {
              "pwsh": "PS",
              "bash": "Bash"
            }
          }
        },
        {
          "type": "session",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "#4C4C4C",
          "background": "#F5D7E3",
          "template": " {{ if .SSHSession }}ÓÆ© {{ end }}{{ .UserName }} "
        },
        {
          "type": "path",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "#4C4C4C",
          "background": "#F4A5AE",
          "properties": {
            "style": "agnoster",
            "folder_separator_icon": "/",
            "home_icon": "~",
            "folder_icon": "\uf07b ..",
            "mapped_locations": {
              "C:\\temp": " Óûô "
            }
          }
        },
        {
          "type": "git",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "#4C4C4C",
          "background": "#F2E86D",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#F2E86D{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#F2E86D{{ end }}",
            "{{ if gt .Ahead 0 }}#A39EC4{{ end }}",
            "{{ if gt .Behind 0 }}#A39EC4{{ end }}"
          ],
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} ÔÅÑ {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} ÔÅÜ {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} ÔÉá {{ .StashCount }}{{ end }}",
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "untracked_modes": {
              "/Users/user/Projects/oh-my-posh/": "no"
            },
            "source": "cli",
            "mapped_branches": {
              "feat/*": "üöÄ ",
              "bug/*": "üêõ "
            }
          }
        },
        {
          "type": "java",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "#4C4C4C",
          "background": "#8AACAB",
          "template": " \ue256 {{ .Full }}"
        },
        {
          "type": "python",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "#C4C19E",
          "background": "#8A98AC",
          "template": " \ue235 {{ .Full }} "
        },
        {
          "type": "rust",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "#4C4C4C",
          "background": "#C49EA0",
          "template": " \uf085 {{ .Full }} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "battery",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "#193549",
          "background": "#ffeb3b",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#40c4ff{{end}}",
            "{{if eq \"Discharging\" .State.String}}#ff5722{{end}}",
            "{{if eq \"Full\" .State.String}}#4caf50{{end}}"
          ],
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}Ôäï ",
          "properties": {
            "discharging_icon": "\ue57a ",
            "charging_icon": "\ue55b ",
            "charged_icon": "\uf06c "
          }
        },
        {
          "type": "time",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "#4C4C4C",
          "background": "#F2D1C9",
          "template": "{{ .CurrentDate | date \" Mon \" }}",
          "properties": {
            "time_format": "15:04:05"
          }
        },
        {
          "type": "time",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "#4C4C4C",
          "background": "#06D6A0",
          "template": "{{ .CurrentDate | date \" 02 \" }}",
          "properties": {
            "time_format": "15:04:05"
          }
        },
        {
          "type": "time",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "#4C4C4C",
          "background": "#06D6A0",
          "template": "{{ .CurrentDate | date \" Jan \" }}",
          "properties": {
            "time_format": "15:04:05"
          }
        },
        {
          "type": "time",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "#4C4C4C",
          "background": "#06D6A0",
          "template": "{{ .CurrentDate | date \" 06 \" }}",
          "properties": {
            "time_format": "15:04:05"
          }
        },
        {
          "type": "time",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "#FFFFFF",
          "background": "#048A81",
          "template": "{{ .CurrentDate | date \" 15:04:05 \" }}",
          "properties": {
            "time_format": "15:04:05"
          }
        },
        {
          "type": "time",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "#FFFFFF",
          "background": "#048A81",
          "template": "{{ .CurrentDate | date \" UTC \" }}",
          "properties": {
            "time_format": "15:04:05"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "leading_diamond": "\u2570\u2500\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "#4C4C4C",
          "background": "#E9E9E9",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "properties": {
            "macos": "MacOS ",
            "windows": "Win ",
            "linux": "\uf17c"
          }
        },
        {
          "type": "status",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "#4C4C4C",
          "background": "#9EC49F",
          "background_templates": ["{{ if .Error }}#C49Ea0{{end}}"],
          "template": "<#193549></> \uf004  ",
          "properties": {
            "always_enabled": true,
            "status_template": "{{ if eq .Code 0 }} \uf00c{{ else }} \uf071 {{ reason .Code }}{{ end }}"
          }
        },
        {
          "type": "text",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "#4C4C4C",
          "background": "#51A3A3",
          "template": " \uf120 "
        }
      ]
    }
  ]
}
