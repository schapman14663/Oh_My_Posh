{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "mapped_shell_names": {
              "bash": "Bash",
              "pwsh": "PS",
              "zsh": "zsh"
            }
          },
          "style": "plain",
          "template": "\u256d\u2500 \uf120 {{ .Name }} ",
          "foreground": "#22E529",
          "type": "shell"
        },
        {
          "style": "plain",
          "template": "\uf007 {{ if .SSHSession }}\ueba9 {{ end }}{{ .UserName }} ",
          "foreground": "#F59574",
          "type": "session"
        },
        {
          "properties": {
            "folder_icon": "\uf07b ..",
            "folder_separator_icon": "/",
            "home_icon": "~",
            "mapped_locations": {
              "C:\\temp": " \ue799 "
            },
            "style": "agnoster"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "foreground": "#F41D99",
          "type": "path"
        },
        {
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "mapped_branches": {
              "bug/*": "üêõ ",
              "feat/*": "\ud83d\ude80 "
            },
            "source": "cli",
            "untracked_modes": {
              "/Users/user/Projects/oh-my-posh/": "no"
            }
          },
          "style": "plain",
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf0c7 {{ .StashCount }}{{ end }}",
          "foreground": "#DD7BDC",
          "type": "git",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#F2E86D{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#F2E86D{{ end }}",
            "{{ if gt .Ahead 0 }}#A39EC4{{ end }}",
            "{{ if gt .Behind 0 }}#A39EC4{{ end }}"
          ]
        },
        {
          "style": "plain",
          "template": " \ue256 {{ .Full }}",
          "foreground": "#BE2D26",
          "type": "java"
        },
        {
          "style": "plain",
          "template": " \ue235 {{ .Full }} ",
          "foreground": "#F2E86D",
          "type": "python"
        },
        {
          "style": "plain",
          "template": " \uf085 {{ .Full }} ",
          "foreground": "#FF5C00",
          "type": "rust"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "charged_icon": "\uf06c ",
            "charging_icon": "\ue55b ",
            "discharging_icon": "\ue57a "
          },
          "style": "plain",
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}\uf295 ",
          "foreground": "#9892F1",
          "type": "battery",
          "foreground_templates": [
            "{{if eq \"Charging\" .State.String}}#40c4ff{{end}}",
            "{{if eq \"Discharging\" .State.String}}#ff5722{{end}}",
            "{{if eq \"Full\" .State.String}}#9892F1{{end}}"
          ]
        },
        {
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "template": "{{ .CurrentDate | date \" Mon \" }}",
          "foreground": "#DD7BDC",
          "type": "time"
        },
        {
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "template": "{{ .CurrentDate | date \" 02 \" }}",
          "foreground": "#F41D99",
          "type": "time"
        },
        {
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "template": "{{ .CurrentDate | date \" Jan \" }}",
          "foreground": "#F41D99",
          "type": "time"
        },
        {
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "template": "{{ .CurrentDate | date \" 06 \" }}",
          "foreground": "#F41D99",
          "type": "time"
        },
        {
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "template": "{{ .CurrentDate | date \" 15:04:05 \" }}",
          "foreground": "#F59574",
          "type": "time"
        },
        {
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "template": "{{ .CurrentDate | date \" UTC \" }}",
          "foreground": "#22e529",
          "type": "time"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "linux": "\uf17c",
            "macos": "MacOS ",
            "windows": "Win "
          },
          "style": "diamond",
          "template": "\u2570\u2500 {{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "foreground": "#E9E9E9",
          "type": "os"
        },
        {
          "properties": {
            "always_enabled": true,
            "status_template": "{{ if eq .Code 0 }} \uf00c{{ else }} \uf071 {{ reason .Code }}{{ end }}"
          },
          "style": "plain",
          "template": "SYSTEMS FINE \uf004  ",
          "foreground": "#31D07B",
          "type": "status",
          "foreground_templates": ["{{ if .Error }}ERROR #BE2D26{{end}}"]
        },
        {
          "style": "plain",
          "template": " \uf054 ",
          "foreground": "#DD7BDC",
          "type": "text"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "auto_upgrade": true,
  "final_space": true
}
